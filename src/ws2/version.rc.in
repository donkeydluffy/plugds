// version.rc.in
#include <windows.h>
#include <winver.h>

LANGUAGE LANG_CHINESE, SUBLANG_DEFAULT

VS_VERSION_INFO VERSIONINFO
 FILEVERSION     @WS2_PRODUCT_VERSION_MAJOR@,@WS2_PRODUCT_VERSION_MINOR@,@WS2_PRODUCT_VERSION_PATCH@,@WS2_PRODUCT_VERSION_TWEAK@
 PRODUCTVERSION  @WS2_PRODUCT_VERSION_MAJOR@,@WS2_PRODUCT_VERSION_MINOR@,@WS2_PRODUCT_VERSION_PATCH@,0
 FILEFLAGSMASK   0x3fL
#ifdef _DEBUG
 FILEFLAGS 0x1L
#else
 FILEFLAGS 0x0L
#endif
 FILEOS          0x40004L
 FILETYPE        0x2L
 FILESUBTYPE     0x0L
BEGIN
    BLOCK "StringFileInfo"
    BEGIN
        BLOCK "040904b0"
        BEGIN
            VALUE "CompanyName", "SCANTECH (HANGZHOU) CO., LTD."
            VALUE "LegalCopyright", "@WS2_PRODUCT_COMPANY_COPYRIGHT@\0"
            VALUE "FileDescription", "@WS2_PRODUCT_NAME@\0"
            VALUE "FileVersion", "@WS2_PRODUCT_FILE_FULL_VERSION@\0"
            VALUE "ProductVersion", "@WS2_PRODUCT_FULL_VERSION@\0"
            VALUE "InternalName", "@WS2_PRODUCT_NAME@\0"
            VALUE "OriginalFilename", "@WS2_PRODUCT_NAME@\0"
            VALUE "ProductName", "@WS2_PRODUCT_NAME@\0"
        END
    END
    BLOCK "VarFileInfo"
    BEGIN
        VALUE "Translation", 0x409, 1200
    END
END